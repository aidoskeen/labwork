<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labwork.Controllers.DormsCtrl">
   <children>
      <ListView fx:id="dormsList" layoutX="12.0" layoutY="179.0" prefHeight="200.0" prefWidth="217.0" />
      <Label layoutX="15.0" layoutY="26.0" text="See rooms of Dormitory" />
      <Button layoutX="121.0" layoutY="103.0" mnemonicParsing="false" onAction="#seeRooms" text="Enter" />
      <TextField fx:id="dormText" layoutX="14.0" layoutY="68.0" />
      <TableView fx:id="dormTable" layoutX="290.0" layoutY="34.0" prefHeight="200.0" prefWidth="303.0">
        <columns>
          <TableColumn fx:id="dormCol" prefWidth="69.0" text="Dormitory" />
          <TableColumn fx:id="addrCol" prefWidth="81.0" text="Address" />
            <TableColumn fx:id="rcountCol" prefWidth="75.0" text="Room count" />
            <TableColumn fx:id="emptyCol" prefWidth="75.0" text="Remove" />
        </columns>
      </TableView>
      <Label layoutX="12.0" layoutY="51.0" text="Dorm id:" />
      <Label layoutX="362.0" layoutY="18.0" text="Dormitories table:" />
      <Button fx:id="goBack" layoutX="518.0" layoutY="354.0" mnemonicParsing="false" onAction="#gobackmain" text="Go back" />
   </children>
</AnchorPane>
